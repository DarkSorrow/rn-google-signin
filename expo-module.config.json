{
  "platforms": [
    "ios",
    "android"
  ],
  "ios": {
    "modules": ["RCTNativeGoogleSignin"],
    "appDelegateSubscribers": ["RCTNativeGoogleSignin"],
    "infoPlist": {
      "GIDClientID": "string"
    }
  },
  "android": {
    "modules": ["RNGoogleSigninModule"],
    "package": "com.novastera.rngooglesignin",
    "buildFeatures": {
      "buildConfig": true
    },
    "gradleProperties": {
      "newArchEnabled": "true"
    }
  },
  "plugin": {
    "name": "rn-google-signin",
    "platforms": ["ios", "android"]
  }
} 