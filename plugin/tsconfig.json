{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "./build",
    "module": "commonjs",
    "target": "es6",
    "lib": [
      "es6"
    ],
    "noEmit": false,
    "declaration": false,
    "rootDir": "./src",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
} 